import turtle
import os 
os.system('cls')

t = turtle.Turtle()
s = turtle.Screen()
s.bgcolor('#262626')
t.pencolor('pink')
t.speed(150)
colors = ('green', 'yellow', '#736AFF', '#A74AC7')
for x in range(7):
    for y in range (8):
        t.speed(y + 18)
        for i in range(2):
            t.pensize(6)
            t.circle(88+x*20,90)
            t.lt(90)
        t.lt(45)
    t.pencolor(colors[x%4])    
s.exitonclick
            